<h1>New Puppy</h1>
<%= form_for @puppy do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :breed %>
    <%= f.text_field :breed %>
  </div>
  <div>
    <%= f.label :age %>
    <%= f.number_field :age %>
  </div>
  <div>
    <%= f.label :tricks %>
    <%= f.check_box :tricks %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
